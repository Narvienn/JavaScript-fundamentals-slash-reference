
const zosArray = [firstName, 'Vonho', 2037 - 1988, ['Daria', 'Krys', 'Asia'], true]; 

//LOOPING BACKWARDS
    for(let i = zosArray.length - 1; i >= 0; i--) {
        console.log(zosArray[i]);
};

//CREATING A LOOP INSIDE A LOOP
for(let exercise = 1; exercise <= 3; exercise++){
    console.log(`------Starting exercise ${exercise}`);
    for(let rep = 1; rep < 6; rep++) {
        console.log(`Lifting weights, repetition no. ${rep}`);
    };

};